{
  "manifest_version": 2,
  "name": "Dark theme for WhatsApp Web",
  "description": "I'll add the dark class to your WhatsApp Web body tag!",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["https://web.whatsapp.com/"],
      "js": ["script.js"]
    }
  ],
  "icons": {
    "64": "icon.png"
  },
  "browser_action": {
    "default_title": "Dark theme for WhatsApp Web"
  }
}
